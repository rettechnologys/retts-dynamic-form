{
  "name": "retts-dynamic-form",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "description": "Dynamic form and base components library for Vue 3 with PrimeVue integration",
  "author": "Retts",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "dynamic-form",
    "form-builder",
    "primevue",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/retts-dynamic-form.git"
  },
  "main": "./dist/retts-dynamic-form.umd.js",
  "module": "./dist/retts-dynamic-form.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/retts-dynamic-form.js",
      "require": "./dist/retts-dynamic-form.umd.js"
    },
    "./types/Form": {
      "types": "./dist/types/Form.d.ts",
      "import": "./dist/types/Form.js",
      "require": "./dist/types/Form.umd.js"
    },
    "./types/SignaturePadTypes": {
      "types": "./dist/types/SignaturePadTypes.d.ts",
      "import": "./dist/types/SignaturePadTypes.js",
      "require": "./dist/types/SignaturePadTypes.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --composite false && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@supercharge/strings": "^2.0.0",
    "@tinymce/tinymce-vue": "^6.0.1",
    "@vueuse/core": "^14.0.0",
    "cronstrue": "^3.9.0",
    "html-to-text": "^9.0.5",
    "juice": "^11.0.3",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^3.53.1",
    "signature_pad": "^5.1.1",
    "tinymce": "^7.3.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.24",
    "vue-codemirror6": "^1.4.1",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.3"
  }
}
